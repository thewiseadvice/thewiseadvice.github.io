      /* about us blixer */
      (function ($) {
        "use strict";
        $(".bxslider").bxSlider({
          mode: "vertical",
          minSlides: 1,
          maxSlides: 1,
          slideMargin: 0,
          pager: false,
          nextText: '<i class="fa fa-arrow-down"></i>',
          prevText: '<i class="fa fa-arrow-up"></i>',
          speed: 1000,
          auto: true,
        });
      })(jQuery);

      /* Flter Portfolio */
      $(function () {
        var filterList = {
          init: function () {
            $("#portfoliolist").mixItUp({
              selectors: {
                target: ".portfolio",
                filter: ".filter",
              },
              load: {
                filter: ".all",
              },
            });
          },
        };
        filterList.init();
      });
